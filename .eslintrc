{
	"env": {
		"browser": true
	},
	"rules": {
		"no-trailing-spaces": 0,
		"eqeqeq": 0,
		"no-console": 1,
        "no-unused-vars": 1,
		"no-shadow": 0,
		"consistent-return": 0,
		"no-cond-assign": 0,
		"quotes": 0,
		"no-use-before-define": [1, "nofunc"],
		"dot-notation": [2, {"allowKeywords": false}]
	},
	"globals": {
		"angular": false,
		"console": false
	}
}
